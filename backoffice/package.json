{
  "name": "backoffice",
  "type": "module",
  "version": "1.0.0",
  "description": "backoffice",
  "main": "src/index.js",
  "author": "Jim Zhan <jim.zhan@me.com>",
  "license": "Apache-2.0",
  "scripts": {
    "db:init": "yarn db:sync",
    "db:sync": "knex migrate:latest",
    "db:new": "knex migrate:make",
    "db:up": "knex migrate:up",
    "db:down": "knex migrate:down",
    "db:rollback": "knex migrate:rollback",
    "lint": "eslint --fix --ignore-path .eslintignore",
    "start": "cross-env NODE_ENV=production node src/index.js",
    "watch": "cross-env NODE_CONFIG_ENV=localhost nodemon src/index.js",
    "pretest": "npm run db:init",
    "test": "cross-env NODE_ENV=test vitest run --coverage --pool=forks"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@hapi/catbox": "^12.1.1",
    "@hapi/catbox-memory": "^6.0.2",
    "@hapi/catbox-redis": "^7.0.2",
    "@hapi/glue": "^9.0.1",
    "@hapi/hapi": "^21.3.10",
    "argon2": "^0.40.3",
    "bson": "^6.8.0",
    "catbox-redis": "^4.2.4",
    "config": "^3.3.12",
    "exiting": "^7.0.0",
    "joi": "^17.13.3",
    "knex": "^3.1.0"
  },
  "devDependencies": {
    "better-sqlite3": "^11.1.2",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.10.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.0.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3"
  }
}
